print("Guess The Number Game")
print("Number of tries is 9 times")

loop = 1
while(loop<9):
    num = int(input("guess the number "))
    if num==10:
        print("You won")
        break
    elif num>20:
        print("Number is bigger!")
        loop = loop + 1
        continue
    elif num<20:
        print("Number is smaller!")
        loop = loop + 1
        continue

else:
    print("You loose")
